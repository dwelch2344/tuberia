{
  "name": "bullmq",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/bullmq/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/bullmq",
        "main": "packages/bullmq/src/index.ts",
        "tsConfig": "packages/bullmq/tsconfig.lib.json",
        "assets": [
          "packages/bullmq/*.md"
        ]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs bullmq {args.ver} {args.tag}",
      "dependsOn": [
        "build"
      ]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/bullmq/**/*.ts",
          "packages/bullmq/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{options.reportsDirectory}"
      ],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/packages/bullmq"
      }
    }
  },
  "tags": []
}